{
    "name": "notekeeper/note-application",
    "description": "A PHP note-taking application with sharing and security features",
    "type": "project",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "require": {
        "ext-pdo": "*",
        "ext-json": "*",
        "phpmailer/phpmailer": "^6.9",
        "spomky-labs/otphp": "^11.3",
        "endroid/qr-code": "^6.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}